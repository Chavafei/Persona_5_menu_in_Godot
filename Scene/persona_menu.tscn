[sub_resource type="GDScript" id="GDScript_iy01n"]
resource_name = "game_state"
script/source = "extends State
@export var main_menu_state : State

@export var open_menu : Juicy_player
func Update(_delta: float):
	if Input.is_action_just_pressed(\"ui_cancel\"):
		Transitioned.emit(self,main_menu_state)
		open_menu.Play()
		
	pass

"

[sub_resource type="GDScript" id="GDScript_qdsg3"]
resource_name = "main_menu_state"
script/source = "extends State
@export var game_state : State
@export var leave_effect : Juicy_player
func Update(_delta: float):
	if Input.is_action_just_pressed(\"ui_cancel\"):
		Transitioned.emit(self,game_state)
		leave_effect.Play()
		
	pass
"

[sub_resource type="GDScript" id="GDScript_jos3y"]
resource_name = "skill_menu_state"
script/source = "extends State

@export var return_to_main : Juicy_player
@export var main_menu_state : State



func Update(_delta: float):
	if Input.is_action_just_pressed(\"ui_cancel\"):
		return_to_main.Play()
		Transitioned.emit(self,main_menu_state)
		
	pass
	

"

